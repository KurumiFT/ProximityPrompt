				<ProtectedString name="Source"><![CDATA[local RunService = game:GetService('RunService')

local ActivityLabel = script.Parent:WaitForChild('ActivityLabel')
local KeyLabel = script.Parent:WaitForChild('KeyLabel')

RunService.PreRender:Connect(function()
	local AbsouluteHeight = script.Parent.AbsoluteSize.Y

	KeyLabel.Size = UDim2.new(0, AbsouluteHeight, 0, AbsouluteHeight)

	ActivityLabel.Size = UDim2.new(.8, -AbsouluteHeight, .8, 0)
	ActivityLabel.Position = UDim2.new(.1, AbsouluteHeight, .1, 0)
end)]]></ProtectedString>
